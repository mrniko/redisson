<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
       xmlns:redisson="http://redisson.org/schema/redisson"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd
       http://redisson.org/schema/redisson classpath:org/redisson/spring/support/redisson-1.1.xsd
">
    <context:annotation-config />
    <context:property-placeholder system-properties-mode="OVERRIDE"/>
    
    <redisson:client id="myRedisson1" name="redisson" >
        <redisson:single-server address="${redisAddress}" />
    </redisson:client>

    <redisson:client id="myOtherRedisson">
        <redisson:single-server address="${redisAddress}" client-name="myOtherRedisson"/>
    </redisson:client>

    <bean class="org.redisson.spring.support.SpringRInjectTest.RInjectRedisson" />
</beans>
